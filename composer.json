{
    "name": "fawno/ip-authentication",
    "description": "IP Authenticator for CakePHP 4.3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "authenticator", "ip", "authentication"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fawno/IPAuthenticator/issues",
        "source": "https://github.com/fawno/IPAuthenticator"
    },
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.3",
        "cakephp/authentication": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "IPAuthenticator\\": "src"
        }
    },
    "prefer-stable": true
}
